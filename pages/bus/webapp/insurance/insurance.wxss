page {
    background: #efeff4;
}

@font-face {
    font-family:iconfont;src:url(//css.40017.cn/cn/train/mobileqq/20160421/iconfont/iconfont.eot);src:url(//css.40017.cn/cn/train/mobileqq/20160421/iconfont/iconfont.eot?) format("embedded-opentype"),url(//css.40017.cn/cn/train/mobileqq/20160421/iconfont/iconfont.ttf) format("truetype"),url(//css.40017.cn/cn/train/mobileqq/20160421/iconfont/iconfont.svg) format("svg");
}

.insurance-box {
    min-height: 100%;
    height: 100%;
}

.insurance {
    line-height: 1;
    padding-bottom: 50px;
}

.list {
    position: relative;
    padding: 0;
    height: 50px;
    border-bottom: 1px solid #eee;
    display: -webkit-box;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}

.nobuy,.text {
    font-size: 16px;
    font-weight: 400;
    width: 100px;
    color: #333;
}

.nobuy {
    color: #666;
}

.nobuy-vou {
    line-height: 60px;
    font-size: 14px;
    color: #999;
}

.text-icon {
    padding: 0 10px 0 6px;
}

.text-icon::after {
    content: '\e61a';
    font-family: iconfont!important;
    font-size: 18px;
    color: #bebebe;
}

.text em {
    position: relative;
    display: inline-block;
    top: -2px;
    background-color: #09bb07;
    padding: 0 6px;
    height: 16px;
    line-height: 16px;
    margin-left: 10px;
    border-radius: 2px;
    font-size: 10px;
    color: #fff;
    font-family: iconfont!important;
}

.info {
    font-size: 12px;
    color: #999;
    padding-right: 30px;
}

.chose {
    position: absolute;
    top: 50%;
    right: 0;
    width: 40px;
    height: 40px;
    margin-top: -20px;
    line-height: 40px;
    text-align: right;
}

.chose::after {
    content: '\e639';
    font-family: iconfont!important;
    font-size: 24px;
    color: #ccc;
}

.select .chose::after {
    content: '\e638';
    color: #2ebd59;
}

.ins-layer {
    position: fixed;
    width: 100%;
    height: 100%;
    padding: 24px;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background: #404040;
    color: #fff;
    box-sizing: border-box;
    z-index: 900;
    overflow-y: scroll;
    -webkit-overflow-scrolling: touch;
}

.title {
    margin-top: 0;
    line-height: 1;
    font-size: 20px;
    text-align: center;
}

.insurance-explain {
    padding-top: 24px;
    font-size: 14px;
    color: #fff;
    padding-bottom: 50px;
    line-height: 20px;
    word-wrap: break-word;
}

.insurance-explain view {
    margin-bottom: 10px;
}

.layer-close {
    width: 100%;
    height: 136px;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 1011;
    background: -webkit-linear-gradient(top,rgba(64,64,64,0) 0,#404040 60%,#404040 100%);
}

.layer-close:before {
    content: '';
    position: absolute;
    left: 50%;
    bottom: 15px;
    width: 28px;
    height: 28px;
    margin-left: -25px;
    border-top: 1px solid #fff;
    transform: rotate(45deg);
}

.layer-close:after {
    content: '';
    position: absolute;
    left: 50%;
    bottom: 35px;
    width: 28px;
    height: 28px;
    margin-left: -25px;
    border-bottom: 1px solid #fff;
    transform: rotate(-45deg);
}

.insurance-explain view {
    margin-bottom: 10px;
}

.insType {
    background-color: #fff;
    margin: 10px;
    border-radius: 5px;
    padding: 0 10px;
    box-shadow: 1px 1px 2px #e6e6e6;
}

.insTitle {
    color: #666;
    padding: 15px 36px 15px 0;
    border-bottom: 1px solid #eee;
    font-size: 17px;
    margin: 0 -10px 0 -10px;
    text-indent: 10px;
    position: relative;
}

.insTitle.isList::after {
    content: '';
    position: absolute;
    top: 50%;
    right: 15px;
    width: 8px;
    height: 8px;
    margin-top: -5px;
    border: 1px solid #bbb;
    transform: rotate(135deg);
    border-width: 1px 1px 0 0;
}

.insTitle.isList.isShrink::after {
    transform: rotate(-45deg);
}

.insTitle.isList.hideIcon::after {
    border: 0;
}

.insTitle.isSwitch {
    position: relative;
}

.insTitle.isSwitch .describe {
    display: block;
    margin-top: 10px;
    color: #999;
    font-size: 12px;
    text-indent: 10px;
}

.insTitle.isSwitch .ricon {
    position: absolute;
    top: 50%;
    margin-top: -14px;
    right: 10px;
}

.quota {
    color: #ff6815;
}

.listUl.isShrink {
    height: 0;
    overflow: hidden;
}

.list:last-child {
    border-bottom: 0;
}

.add-ok {
    background-color: #00bb07;
    height: 44px;
    line-height: 44px;
    text-align: center;
    font-size: 18px;
    color: #fff;
    z-index: 1;
    position: fixed;
    bottom: 0;
    width: 100%;
}

.ricon {
    background-color: #fff;
    width: 48px;
    height: 28px;
    border: 1px solid #e6e6e6;
    border-radius: 999px;
    box-shadow: 1px 1px 1px #efefef;
    -webkit-transition-duration: .2s;
    -webkit-transition-property: background-color,border;
}

.ricon:after {
    content: '';
    display: block;
    background-color: #fff;
    width: 26px;
    height: 26px;
    margin-left: -1px;
    border-radius: 50%;
    -webkit-transition-duration: .2s;
    -webkit-transition-property: transform,margin-left;
    border: 1px solid #e6e6e6;
    pointer-events: none;
    margin-top: -1px;
}

.ricon-check {
    box-shadow: 1px 1px 1px #86dc89;
    background-color: #00bb07;
    border: 1px solid #00bb07;
}

.ricon-check:after {
    -webkit-transform: translateX(21px);
    margin-left: 0;
    border: 1px solid #00bb07;
}

.flex-box {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    align-content: center;
}

.flex1 {
    -webkit-box-flex: 1;
    flex: 1;
}

.close-box {
    width: 100%;
    height: 100px;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 1011;
    background: -webkit-linear-gradient(top,rgba(64,64,64,0) 0,#404040 60%,#404040 100%);
}

.close-link {
    text-decoration: underline;
    color: #ff6815;
    font-size: 20px;
    text-align: center;
    line-height: 146px;
}

.close:after,.close:before {
    content: '';
    height: 2px;
    width: 40px;
    background-color: #fff;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -21px;
}

.close:after {
    transform: rotate(45deg);
}

.close:before {
    transform: rotate(-45deg);
}

.close1 {
    height: 80px;
    width: 80px;
    position: relative;
    margin-top: 34px;
}