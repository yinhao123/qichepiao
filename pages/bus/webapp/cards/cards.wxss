@charset "UTF-8";

@font-face {
    font-family:iconfont;src:url(//css.40017.cn/cn/train/mobileqq/20160421/iconfont/iconfont.eot);src:url(//css.40017.cn/cn/train/mobileqq/20160421/iconfont/iconfont.eot?) format("embedded-opentype"),url(//css.40017.cn/cn/train/mobileqq/20160421/iconfont/iconfont.ttf) format("truetype"),url(//css.40017.cn/cn/train/mobileqq/20160421/iconfont/iconfont.svg) format("svg");
}

.flex-box {
    display: -webkit-box;
    display: flex;
}

.flex1 {
    -webkit-box-flex: 1;
    flex: 1;
}

.tab-wrap {
    width: 100%;
    height: 46px;
    border-bottom: 1px solid #dcdcdc;
}

.tab-wrap view {
    text-align: center;
    line-height: 46px;
    color: #666;
    font-size: 16px;
    position: relative;
}

.tab-wrap view.selected::after {
    content: "";
    display: block;
    position: absolute;
    bottom: 0;
    height: 2px;
    background-color: #09bb07;
    width: 60%;
    left: 20%;
}

.card-box {
    margin-top: 10px;
}

.submit-btn {
    width: 100%;
    height: 44px;
    font-size: 18px;
    line-height: 44px;
    color: #fff;
    text-align: center;
    position: fixed;
    bottom: 0;
    left: 0;
}

.card-box .no-use {
    height: 55px;
    border-top: 1px solid #edeef1;
    border-bottom: 1px solid #edeef1;
    padding: 0 15px;
    font-size: 14px;
    color: #999;
    line-height: 55px;
    background-color: #fff;
    box-shadow: 0 1px 1px #f2f2f2;
}

.no-use {
    font-size: 14px;
    color: #999;
}

.card-item.selected .chose {
    width: 22px;
    height: 22px;
    line-height: 55px;
    position: absolute;
    right: 10px;
    top: 50%;
    margin-top: -11px;
    background: transparent url(http://file.40017.cn/img140017cnproduct/touch/bus/wx_bus/caed_checked.png) no-repeat scroll 0 0;
    background-size: 100% 100%;
}

.no-use view:nth-child(2) {
    height: 55px;
    padding-left: 10px;
    line-height: 55px;
    font-size: 15px;
}

.can-use {
    padding: 0 15px 0 12px;
}

.card-item {
    height: 100px;
    border-top: 1px solid #edeef1;
    border-bottom: 1px solid #edeef1;
    font-size: 14px;
    color: #999;
    background-color: #efeff4;
    margin-top: 10px;
    position: relative;
    overflow: hidden;
}

.card-item .price-wrap {
    width: 30%;
    color: #ff6815;
    background: -webkit-linear-gradient(225deg,#16c59e,#2fd589);
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-pack: center;
    justify-content: center;
    text-align: center;
}

.reject-use .price-wrap {
    color: #999;
}

.card-item .price-wrap text:first-child {
    height: 20px;
    margin-top: 10px;
    line-height: 20px;
}

.card-item .price-wrap text:nth-child(2) {
    height: 12px;
    font-size: 12px;
    line-height: 12px;
    color: #999;
    margin-top: 3px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.card-item .price-wrap text text:first-child {
    font-size: 14px;
    color: #ff6815;
}

.card-item .price-wrap text text:nth-child(2) {
    font-size: 20px;
    color: #ff6815;
}

.no-more {
    width: 100%;
    height: 12px;
    line-height: 12px;
    padding: 0 15px;
    margin-top: 10px;
    font-size: 12px;
    text-align: center;
    color: #b6b6b6;
    box-sizing: border-box;
}

.no-more .left-line,.no-more .right-line {
    width: 28%;
    height: 1px;
    background-color: #b6b6b6;
    margin-top: 6px;
}

.no-more .center-text {
    width: 100%;
    text-align: center;
}

.without-bg {
    width: 100%;
}

.with-out-img {
    margin-top: 66px;
}

.with-out-img image {
    display: block;
    width: 45%;
    margin: 0 auto;
}

.with-out-img view {
    text-align: center;
}

view.none {
    display: none;
}

.css-circle-wrap {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    justify-content: space-around;
    position: absolute;
    left: 0;
    height: 98px;
    padding: 2px 0;
}

.circle {
    width: 3px;
    height: 6px;
    border-radius: 0 3px 3px 0;
    background-color: #e2e4eb;
}

.intro-wrap {
    background-color: #fff;
    -webkit-box-flex: 1;
    flex: 1;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-pack: center;
    justify-content: center;
    position: relative;
}

.card-item .circle,.card-item .intro-wrap,.card-item .price-wrap {
    border: 1px solid transparent;
}

.card-item.selected .circle,.card-item.selected .intro-wrap,.card-item.selected .price-wrap {
    border: 1px solid #09ba07;
}

.card-item.selected .circle,.card-item.selected .intro-wrap {
    border-left: 0;
}

.price-box {
    font-size: 38px;
    color: #fff;
    font-weight: 700;
}

.price-box text {
    font-size: 16px;
    margin-right: 5px;
}

.rule {
    color: #fff;
    padding-top: 2px;
}

.intro {
    padding: 0 42px 0 20px;
    font-size: 14px;
    height: 32px;
    line-height: 16px;
}

.expire {
    padding: 10px 42px 0 20px;
    color: #333;
    font-size: 15px;
}

.cannot-use {
    padding: 0 15px 0 12px;
}

.reject-use .state {
    width: 70px;
    height: 60px;
    position: absolute;
    right: 4px;
    top: 8px;
    background: transparent url(http://file.40017.cn/img140017cnproduct/touch/bus/wx_bus/icon-old.png) no-repeat scroll 0 0;
    background-size: 100% auto;
}

.reject-use .state.pastdue {
    background: transparent url(http://file.40017.cn/img140017cnproduct/touch/bus/wx_bus/icon-old.png) no-repeat scroll 0 0;
    background-size: 100% auto;
}

.reject-use .state.notstart {
    background: transparent url(http://file.40017.cn/img140017cnproduct/touch/bus/wx_bus/icon-nostart.png) no-repeat scroll 0 0;
    background-size: 100% auto;
}

.reject-use .state.notmatch {
    background: transparent url(http://file.40017.cn/img140017cnproduct/touch/bus/wx_bus/icon-nomatch.png) no-repeat scroll 0 0;
    background-size: 100% auto;
}

.reject-use .price-wrap {
    background: -webkit-linear-gradient(225deg,#dedede,#d1d2d4);
}

.reject-use .intro {
    color: #999;
}

.reject-use .expire {
    color: #ccc;
    font-size: 15px;
}

.tipsTitle {
    color: #666;
    font-size: 20px;
    margin-top: -34px;
}

.tipsDesc {
    color: #999;
    font-size: 14px;
    margin-top: 10px;
}

.eitherOrAct {
    position: fixed;
    bottom: 50%;
    text-align: center;
    width: 80%;
    background-color: rgba(0,0,0,.8);
    color: #fff;
    left: 10%;
    padding: 14px 0;
    font-size: 15px;
}